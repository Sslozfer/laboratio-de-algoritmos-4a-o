while True: 
    edad = int(input("ingrese su edad: "))
    if edad  < 3:
        print("su entrada es gratis")
    elif  12 > edad >= 3:
        print("su entrada cuesta 10")
    elif  edad >= 12:
        print("su entrada cuesta 15")
    continuar = input("desea continuar?")
    if continuar == "no":       
        break